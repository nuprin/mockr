<div id="feedback_header">
  <ul id="stats_list">
    <li class="happy" id="happy_count"
      title="<%= pluralize(@mock.happy_count, "happy reaction") %>"><%=
      @mock.happy_count %></li>
    <li class="sad" id="sad_count"
      title="<%= pluralize(@mock.sad_count, "sad reaction") %>"><%=
      @mock.sad_count %></li>
  </ul>
  <h3>
    <%= link_to_function "Expand All", "mockr.expandAll()" %>
    -
    <%= link_to_function "Collapse All", "mockr.collapseAll()" %>
  </h3>
</div>
